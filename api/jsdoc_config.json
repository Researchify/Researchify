{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "exclude": ["./api/src/controllers/foo.js"]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "template": "node_modules/docdash"
    },
    "docdash": {
        "search": true
    }
}