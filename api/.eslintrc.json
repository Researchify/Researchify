{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": "off",
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "prefer-destructuring": "off",
        "no-use-before-define": ["error", {"functions":  false, "classes":  false}]
    }
}
